[ignore]
.*/node_modules/styled-components/.*


[untyped]
.*/node_modules/*

[include]

[libs]
./flow-typed

[options]
emoji=true

module.file_ext=.js
module.file_ext=.native.js
module.file_ext=.ios.js
module.file_ext=.android.js

module.ignore_non_literal_requires=true
suppress_type=$FlowIssue

module.name_mapper='^.\(.*\)$' -> '<PROJECT_ROOT>/utils/\1'
module.name_mapper='^.\(.*\)$' -> '<PROJECT_ROOT>/reducers/\1'
module.name_mapper='^.\(.*\)$' -> '<PROJECT_ROOT>/components/\1'

[lints]
untyped-type-import=warn


[strict]
deprecated-type
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import

[version]
^0.129.0